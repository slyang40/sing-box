{
    "log": {
        "disabled": false,
        "level": "warn",
        "output": "/var/log/sing-box.log",
        "timestamp": true
    },
    
    "ntp": {
        "enabled": true,
        "server": "192.168.30.3",
        "server_port": 123,
        "interval": "5m"
    },
    
    "experimental": {
        "clash_api": {
            "external_controller": "0.0.0.0:9090",
            "external_ui": "/etc/sing-box/ui",
            "secret": "",
